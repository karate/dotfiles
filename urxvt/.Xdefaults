URxvt*font:              xft:DejaVuSansMono:style=Book:size=10:antialias=True:hinting=true \
                         xtf:PowerlineSymbols:size=10:antialias=True \
                         xtf:NotoColorEmoji:size=10:antialias=True \
URxvt*boldFont:          xft:DejaVuSansMono:style=Bold:size=10:antialias=True:hinting=true \
                         xtf:PowerlineSymbols:size=10:antialias=True
                         xtf:NotoColorEmoji:size=10:antialias=True \
URxvt*urgentOnBell:     true
URxvt*termName:         rxvt-unicode-256color
URxvt*letterSpace:      0
URxvt*scrollBar:        false
URxvt*perl-lib:         /usr/lib/urxvt/perl/
URxvt*perl-ext-common:  default,matcher,selection-to-clipboard
!URxvt.keysym.Control-Shift-c:     perl:pasta:copy
!URxvt.keysym.Control-Shift-V:     perl:pasta:paste

URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard

Urxvt*urlLauncher:      /usr/bin/firefox
Urxvt*matcher.button:   1
Urxvt*fading:           20
!*fadeColor:             #000000
Urxvt*visualBell:       true

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

URxvt.keysym.Home: \033[H
URxvt.keysym.End: \033[F
URxvt.keysym.KP_Home: \033[1~
URxvt.keysym.KP_End:  \033[4~
!URxvt.keysym.Home: \033[H
!URxvt.keysym.End: \033[F
!URxvt.keysym.KP_Home: \033[H
!URxvt.keysym.KP_End:  \033[F
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C

urxvt*background:       #282828
urxvt*depth:            24
!urxvt*background:      rgba:0000/0000/0000/cccc

!URxvt.transparent:   true
!URxvt.tintColor:     #282828
!URxvt.shading:      99

!*.foreground:   #E6E1DC
*.foreground:   #FFFFFF
*.background:   #282828

!URxvt.color12: rgb:5c/5c/ff

*.cursorColor:  #D8DEE9
! black
*.color0: #000000
*.color8: #505050
! red
*.color1: #DA4939
*.color9: #FF5844
! green
*.color2: #519F50
*.color10: #83D182
! yellow
*.color3: #FFD24A
*.color11: #FFFF7C
! blue
*.color4: #6D9CBE
*.color12: #9FCEF0
! magenta
*.color5: #D0D0FF
*.color13: #FFFFFF
! cyan
*.color6: #6E9CBE
*.color14: #A0CEF0
! white
*.color7: #FFFFFF
*.color15: #FFFFFF
